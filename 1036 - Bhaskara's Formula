import math

V=input().split()

A=float(V[0])
B=float(V[1])
C=float(V[2])

D=(B**2)-4*A*C

if D<=0:
  print("Impossivel calcular")

else:

  if A ==0:
    print("Impossivel calcular")
    
  else:
    S= math.sqrt(D)

    x1=((-B)+S)/(2*A)
    x2=((-B)-S)/(2*A)
  
    print(f"R1 = {x1:.5f}\nR2 = {x2:.5f}")
